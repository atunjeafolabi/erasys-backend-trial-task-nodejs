{
    "name": "erasys-password-checker-cli",
    "version": "0.1.0",
    "description": "An API endpoint that checks if a password is valid and a CLI application that check for compromised passwords",
    "main": "src/server.ts",
    "directories": {
      "test": "test"
    },
    "scripts": {
      "start": "npm run dev",
      "build": "tsc -p .",
      "dev": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
      "test": "jest -c ./jest.config.json"
    },
    "keywords": [
      "API",
      "CLI",
      "Password",
      "Checker"
    ],
    "author": "Olatunji Afolabi",
    "license": "ISC",
    "dependencies": {
      "dotenv": "^10.0.0",
      "express": "^4.17.1",
      "http-status-codes": "^2.1.4",
      "reflect-metadata": "^0.1.13",
      "typedi": "^0.10.0",
      "typeorm": "^0.2.37"
    },
    "devDependencies": {
      "@types/express": "^4.17.13",
      "@types/jest": "^27.0.1",
      "@types/node": "^16.7.10",
      "@types/supertest": "^2.0.11",
      "jest": "^27.1.0",
      "nodemon": "^2.0.12",
      "prettier": "2.3.2",
      "supertest": "^6.1.6",
      "ts-jest": "^27.0.5",
      "ts-node": "^10.2.1",
      "typescript": "^4.4.2",
      "xo": "^0.44.0"
    }
  }
  